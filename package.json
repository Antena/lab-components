{
  "name": "lab-components",
  "version": "1.2.7",
  "description": "Components for visualizing FHIR labs.",
  "main": "src/index.js",
  "author": "Antena",
  "maintainers": [
    "Denise Fernandez <denise@antena.co>",
    "Gonzalo Bellver <gonzalo@antena.co>"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/Antena/lab-components.git"
  },
  "scripts": {
    "lint": "jshint . --reporter=node_modules/jshint-stylish; jscs . --reporter=node_modules/jscs-stylish",
    "docs": "npm run build && grunt docs",
    "test": "npm run test-unit && npm run test-functional",
    "test-unit": "karma start",
    "test-functional": "rm -rf test/visual/screenshots/diff && wdio wdio.conf.js",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && webpack",
    "watch": "npm run clean && webpack --watch",
    "run-express": "nodemon server --watch server -e js,html,json",
    "start": "parallelshell 'npm run -s run-express' 'npm run -s watch'",
    "dev": "nodemon server.js"
  },
  "dependencies": {
    "angular": "~1.4.x",
    "angular-animate": "~1.4.x",
    "angular-fhir-utils": "0.x",
    "angular-sanitize": "~1.4.x",
    "angular-scroll": "~1.0.0",
    "angular-translate": "~2.7.2",
    "angular-ui-bootstrap": "^2.0.1",
    "angular-ui-router": "^0.2.15",
    "angular-utilities": "0.0.7",
    "antena-slideout": "^0.1.14",
    "bootstrap-sass": "~3.3.6",
    "d3": "~3.5.17",
    "jquery": "~2.1.1",
    "lodash": "~4.12.0",
    "moment": "2.8.1",
    "susy": "~2.2.12",
    "underscore": "~1.8.3"
  },
  "devDependencies": {
    "absolut-loader": "~0.0.5",
    "angular-mocks": "1.5.0",
    "chai": "^3.5.0",
    "css-loader": "0.21.0",
    "express": "^4.12.3",
    "extract-text-webpack-plugin": "2.1.2",
    "grunt": "^1.0.2",
    "grunt-cli": "1.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-ngdocs": "~0.2.10",
    "html-loader": "0.4.3",
    "jscs": "^2.6.0",
    "jscs-stylish": "^0.3.1",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.1.0",
    "json-loader": "0.5.7",
    "karma": "^1.1.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.0.1",
    "ng-annotate-loader": "^0.6.1",
    "ng-annotate-patched": "^1.7.0",
    "ngtemplate-loader": "~2.0.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "node-sass": "4.7.2",
    "node-sass-importer": "1.0.0",
    "nodemon": "1.3.7",
    "parallelshell": "^3.0.2",
    "sass-loader": "6.0.5",
    "style-loader": "0.13.0",
    "uglifyjs-webpack-plugin": "^1.0.0",
    "url-loader": "^0.5.7",
    "wdio-mocha-framework": "^0.5.8",
    "wdio-selenium-standalone-service": "0.x",
    "wdio-spec-reporter": "0.0.5",
    "wdio-visual-regression-service": "^0.6.0",
    "webdriverio": "^4.6.1",
    "webpack": "2.7.0",
